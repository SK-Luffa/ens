EAAsB;AAClB,WAAO/B,UAAU,CAACC,GAAD,EAAM;AAACqB,MAAAA,IAAI,EAAEQ;AAAP,KAAN,EAAqB,KAArB,EAA4B1B,OAA5B,EAAqCC,EAArC,EAAyCC,OAAzC,CAAjB;AACH;;AACD,MAAIyB,MAAM,KAAK,QAAf,EAAyB;AACrB,WAAO/B,UAAU,CAACC,GAAD,EAAM;AAACC,MAAAA,MAAM,EAAE4B;AAAT,KAAN,EAAuB,QAAvB,EAAiC1B,OAAjC,EAA0CC,EAA1C,EAA8CC,OAA9C,CAAjB;AACH;;AACD,SAAON,UAAU,CAACC,GAAD,EAAM;AAACC,IAAAA,MAAM,EAAE4B;AAAT,GAAN,EAAuB,KAAvB,EAA8B1B,OAA9B,EAAuCC,EAAvC,EAA2CC,OAA3C,CAAjB,CAd8E,CAcR;AACzE,CAfD;;AAiBAuB,OAAO,CAACI,GAAR,GAAc,UAAChC,GAAD,EAAM6B,KAAN,EAAa1B,OAAb,EAAsBC,EAAtB;AAAA,SACVwB,OAAO,CAAC;AAAE1B,IAAAA,MAAM,EAAE,KAAV;AAAiBF,IAAAA,GAAG,EAAHA,GAAjB;AAAsB6B,IAAAA,KAAK,EAALA,KAAtB;AAA6B1B,IAAAA,OAAO,EAAPA,OAA7B;AAAsCC,IAAAA,EAAE,EAAFA;AAAtC,GAAD,CADG;AAAA,CAAd;;AAEAwB,OAAO,CAACK,IAAR,GAAe,UAACjC,GAAD,EAAM6B,KAAN,EAAa1B,OAAb,EAAsBC,EAAtB;AAAA,SACXwB,OAAO,CAAC;AAAE1B,IAAAA,MAAM,EAAE,MAAV;AAAkBF,IAAAA,GAAG,EAAHA,GAAlB;AAAuB6B,IAAAA,KAAK,EAALA,KAAvB;AAA8B1B,IAAAA,OAAO,EAAPA,OAA9B;AAAuCC,IAAAA,EAAE,EAAFA;AAAvC,GAAD,CADI;AAAA,CAAf;;AAEAwB,OAAO,CAACM,GAAR,GAAc,UAAClC,GAAD,EAAM6B,KAAN,EAAa1B,OAAb,EAAsBC,EAAtB;AAAA,SACVwB,OAAO,CAAC;AAAE1B,IAAAA,MAAM,EAAE,KAAV;AAAiBF,IAAAA,GAAG,EAAHA,GAAjB;AAAsB6B,IAAAA,KAAK,EAALA,KAAtB;AAA6B1B,IAAAA,OAAO,EAAPA,OAA7B;AAAsCC,IAAAA,EAAE,EAAFA;AAAtC,GAAD,CADG;AAAA,CAAd;;AAEAwB,OAAO,CAACO,MAAR,GAAiB,UAACnC,GAAD,EAAM6B,KAAN,EAAa1B,OAAb,EAAsBC,EAAtB;AAAA,SACbwB,OAAO,CAAC;AAAE1B,IAAAA,MAAM,EAAE,QAAV;AAAoBF,IAAAA,GAAG,EAAHA,GAApB;AAAyB6B,IAAAA,KAAK,EAALA,KAAzB;AAAgC1B,IAAAA,OAAO,EAAPA,OAAhC;AAAyCC,IAAAA,EAAE,EAAFA;AAAzC,GAAD,CADM;AAAA,CAAjB;;AAGA,eAAewB,OAAf","sourcesContent":["/* eslint-disable no-unused-expressions */\n/* eslint-disable no-unused-vars */\n/* eslint-disable no-undef */\n/* eslint-disable no-alert */\n/* eslint-disable no-console */\n/**\n        |--------------------------------------------------\n        | 封装的异步请求函数\n        |--------------------------------------------------\n        */\n// import axios from \"axios\";\nimport config from \"./config\";\n\n// Message.config({\n//   duration: 1500,\n//   // maxCount: 1,\n// });\n// 实例对象\nconst message = {\n    error: function (txt) {\n\n    }\n};\n\nconst AJAX_TIMEOUT = \"ECONNABORTED\";\n// console.log(window.root_config)\nlet ROOT_URL = \"\";\n\nif (process.env.NODE_ENV === \"development\") {\n    ROOT_URL = config.development;\n} else {\n    ROOT_URL = config.product;\n}\n\nconst newRequest = (url, params, method, onError, vm, loading) =>\n    new Promise((resolve, reject) => {\n\n\n        if(vm) {\n            vm.$store.state.tipsb = true;\n            if (loading == \"off\") {\n                vm.$store.state.tipsb = false\n            }\n        }\n\n        let postData = {};\n        // console.log(localStorage.getItem(\"user\"));\n\n        // const user = JSON.parse(localStorage.getItem(\"user\"));\n\n        let turl = ROOT_URL + url;\n        postData = {\n            url: turl,\n            method,\n            timeout: 60000,\n            withCredentials: false,\n            headers: {\n                \"Access-Token\": localStorage.token,\n                \"Access-Control-Allow-Origin\": \"*\"\n            },\n            ...params\n        };\n\n\n        axios(postData)\n            .then(({ data }) => {\n\n                if(vm) {\n                    vm.$store.state.tipsb = false;\n                }\n                //   debugger\n                // if (data.code == 601) {\n                //     localStorage.user = \"\";\n                //     localStorage.token = \"\"\n                //     window.location.href = \"/\"\n\n                // }\n\n                const {code, msg, user} = data;\n\n                if (code == 200 && user == null) {\n                    localStorage.user = \"\";\n                    // this.$store.state.blank = \"\";\n\n                    if(vm) {\n                        vm.$store.state.login_bool = false;\n                    }\n\n\n                    localStorage.token = \"\"\n                    // vm.$store.state.Navigation_in = \"Navigation_in\";\n                    // this.$router.push(\"/\");\n                } else if (code == 680) {\n                    localStorage.token = \"\"\n                }\n\n                resolve(data);\n            })\n            .catch(error => {\n\n                if(vm) {\n                    vm.$store.state.tipsb = false;\n                    vm.$message.error(error)\n                }\n                onError && onError(error);\n\n                // if (error.code === AJAX_TIMEOUT) {\n                //   message.error(vm.$t('message.MESSAGE.code.timeOut'));\n                // } else {\n                //   message.error(vm.$t('message.MESSAGE.code.error'));\n                //   throw error;\n                // }\n            });\n    });\nconst request = ({url = \"\", param = {}, method = \"get\", onError, vm, loading}) => {\n\n\n    const Method = method.toLowerCase();\n    if (Method === \"post\") {\n        // console.log(\"post\", param);\n        return newRequest(url, {data: param}, \"post\", onError, vm, loading);\n    }\n    if (Method === \"put\") {\n        return newRequest(url, {data: param}, \"put\", onError, vm, loading);\n    }\n    if (Method === \"delete\") {\n        return newRequest(url, {params: param}, \"delete\", onError, vm, loading);\n    }\n    return newRequest(url, {params: param}, \"get\", onError, vm, loading); // 默认 Get 请求\n};\n\nrequest.get = (url, param, onError, vm) =>\n    request({ method: \"get\", url, param, onError, vm });\nrequest.post = (url, param, onError, vm) =>\n    request({ method: \"post\", url, param, onError, vm });\nrequest.put = (url, param, onError, vm) =>\n    request({ method: \"put\", url, param, onError, vm });\nrequest.delete = (url, param, onError, vm) =>\n    request({ method: \"delete\", url, param, onError, vm });\n\nexport default request;"]}]}